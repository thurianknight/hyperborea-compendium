":0,"proficiencies":"","ca":{"value":0,"temp":0},"saves":{"death":{"value":14},"device":{"value":14},"transformation":{"value":14},"avoidance":{"value":14},"sorcery":{"value":"14"},"wand":{"value":11},"paralysis":{"value":12},"breath":{"value":13},"spell":{"value":"14"}},"movement":{"base":20,"value":""},"initiative":{"value":0,"mod":0},"spells":{"1":{"max":0},"2":{"max":0},"3":{"max":0},"4":{"max":0},"5":{"max":0},"6":{"max":0},"enabled":false},"details":{"biography":"<div class=\"page\" title=\"Page 33\">\n<div class=\"layoutArea\">\n<div class=\"column\">\n<p>Insects of prodigious size; each may be the product of sorcerous tampering.</p>\n<div class=\"page\" title=\"Page 34\">\n<div class=\"layoutArea\">\n<div class=\"column\">\n<p>STAG BEETLE: The stag beetle is the most enormous of giant beetles, with a body of 12-foot length and horns that project another 8&ndash;10 feet. Heavily armoured and given little cause to fear most predators, these enormous insects are the bane of crop farmers, oft consuming an entire harvest in one night (though they attack humans only if incited). They typically travel alone or in pairs.</p>\n</div>\n</div>\n</div>\n</div>\n</div>\n</div>","size":"L","alignment":"N","xp":"340","specialAbilities":"0","treasure":{"table":"","type":""},"appearing":{"d":"1d2","w":"1d2"},"morale":"7"},"attacks":"","encumbrance":{},"config":{},"languages":{}},"prototypeToken":{"name":"Beetle Giant, Stag","displayName":30,"actorLink":false,"width":1,"height":1,"lockRotation":false,"rotation":0,"alpha":1,"light":{"alpha":0.5,"angle":360,"bright":0,"coloration":1,"dim":0,"luminosity":0.5,"saturation":0,"contrast":0,"shadows":0,"animation":{"speed":5,"intensity":5,"reverse":false,"type":null},"darkness":{"min":0,"max":1},"attenuation":0.5,"color":null},"disposition":0,"displayBars":30,"bar1":{"attribute":"hp"},"bar2":{"attribute":null},"flags":{},"randomImg":false,"texture":{"src":"https://assets.forge-vtt.com/606e5fd2a40de9e2afbe2945/tokenizer/npc-images/bee